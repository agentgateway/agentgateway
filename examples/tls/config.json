{
  "type": "static",
  "listeners": [
    {
      "sse": {
        "address": "[::]",
        "port": 3000,
        "tls": {
          "cert_pem": {
            "file_path": "examples/tls/certs/cert.pem"
          },
          "key_pem": {
            "file_path": "examples/tls/certs/key.pem"
          }
        }
      }
    }
  ],
  "targets": {
    "mcp": [
      {
        "name": "everything",
        "stdio": {
          "cmd": "npx",
          "args": [
            "@modelcontextprotocol/server-everything"
          ]
        }
      }
    ]
  }
}