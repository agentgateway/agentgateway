{
  "name": "agentgateway-ui",
  "version": "0.1.0",
  "private": false,
  "description": "UI for agentgateway",
  "main": "index.js",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npm run lint -- --fix && next build",
    "start": "next start",
    "lint": "next lint",
    "prepublishOnly": "npm run build",

    "// For New Developers": "Complete guided setup experience",
    "test:e2e:setup-wizard": "node ../scripts/setup-wizard.js",
    "test:e2e:smart": "../scripts/run-e2e-tests.sh",
    "test:e2e:error-recovery": "node ../scripts/enhanced-error-recovery.js",

    "// For Experienced Developers": "Quick expert setup and advanced configuration",
    "test:e2e:setup-wizard:quick": "node ../scripts/setup-wizard.js --quick",
    "test:e2e:smart-defaults:template": "node ../scripts/smart-defaults-system.js --create-template",
    "test:e2e:smart-defaults:speed": "node ../scripts/smart-defaults-system.js --prefer-speed",

    "// For System Administrators": "Comprehensive health checks and system analysis",
    "test:e2e:health-check:verbose": "node ../scripts/health-check-validator.js --verbose",
    "test:e2e:smart-defaults:check": "node ../scripts/smart-defaults-system.js --check-only",
    "test:e2e:error-recovery:analyze": "node ../scripts/enhanced-error-recovery.js --analyze-log",

    "// Journey Management": "Manual CI/CD trigger support with journey selection",
    "test:e2e:journey:list": "node ../scripts/journey-manager.js --list",
    "test:e2e:journey:validate": "node ../scripts/journey-manager.js --validate-only --journey-selection=quick-validation",
    "test:e2e:journey:dry-run": "node ../scripts/journey-manager.js --dry-run --journey-selection=quick-validation",
    "test:e2e:journey:smoke": "node ../scripts/journey-manager.js --execute --journey-selection=smoke",
    "test:e2e:journey:quick": "node ../scripts/journey-manager.js --execute --journey-selection=quick-validation",
    "test:e2e:journey:feature": "node ../scripts/journey-manager.js --execute --journey-selection=feature-testing",
    "test:e2e:journey:comprehensive": "node ../scripts/journey-manager.js --execute --journey-selection=comprehensive",
    "test:e2e:journey:custom": "node ../scripts/journey-manager.js --execute --journey-selection=custom --custom-journeys=smoke,foundation",

    "// Traditional Setup (Alternative)": "Manual setup and legacy workflows",
    "e2e:open": "cypress open",

    "// For Test Writers": "Advanced testing tools and configuration",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:headless": "cypress run --headless",
    "cypress:run:headed": "cypress run --headed",
    "cypress:run:chrome": "cypress run --browser chrome",
    "cypress:run:firefox": "cypress run --browser firefox",
    "cypress:run:edge": "cypress run --browser edge",
    "e2e": "cypress run",
    "e2e:smoke": "cypress run --spec 'cypress/e2e/smoke/**/*.cy.{js,jsx,ts,tsx}'",
    "e2e:smoke:headed": "cypress run --headed --spec 'cypress/e2e/smoke/**/*.cy.{js,jsx,ts,tsx}'",
    "e2e:smoke:with-backend": "./scripts/run-smoke-tests.sh",
    "test:e2e:parallel": "node ../scripts/parallel-test-runner.js",
    "test:e2e:parallel:dev": "node ../scripts/parallel-test-runner.js --dev",
    "test:e2e:parallel:ci": "node ../scripts/parallel-test-runner.js --ci",
    "test:e2e:parallel:ci:headed": "node ../scripts/parallel-test-runner.js --ci --headed",
    "test:e2e:parallel:debug": "node ../scripts/parallel-test-runner.js --debug --workers=1",
    "test:e2e:parallel:chrome": "node ../scripts/parallel-test-runner.js --browser chrome",
    "test:e2e:parallel:firefox": "node ../scripts/parallel-test-runner.js --browser firefox",
    "test:e2e:parallel:smoke": "node ../scripts/parallel-test-runner.js --smoke",
    "test:e2e:conservative": "node ../scripts/parallel-test-runner.js --workers=2 --memory-limit=70",
    "test:e2e:sequential": "cypress run",
    "test:e2e:sequential:headed": "cypress run --headed",
    "test:e2e:optimized": "node ../scripts/intelligent-test-config.js && ../scripts/run-e2e-tests.sh --config test-config-optimized.yaml",
    "test:e2e:auto-config": "node ../scripts/intelligent-test-config.js",
    "test:e2e:check-config": "node ../scripts/intelligent-test-config.js --check-only",
    "test:e2e:force-config": "node ../scripts/intelligent-test-config.js --force",
    "test:e2e:health-check": "node ../scripts/health-check-validator.js",
    "test:e2e:health-check:runtime": "node ../scripts/health-check-validator.js --include-runtime --verbose",
    "test:e2e:smart-defaults": "node ../scripts/smart-defaults-system.js",
    "test:e2e:smart-defaults:force": "node ../scripts/smart-defaults-system.js --force",
    "test:e2e:smart-defaults:stability": "node ../scripts/smart-defaults-system.js --prefer-stability",
    "test:e2e:smart:sequential": "../scripts/run-e2e-tests.sh --mode sequential",
    "test:e2e:smart:interactive": "../scripts/run-e2e-tests.sh --mode interactive",
    "test:e2e:smart:verbose": "../scripts/run-e2e-tests.sh --verbose",
    "test:e2e:smart:headed": "../scripts/run-e2e-tests.sh --headed",
    "test:e2e:setup-wizard:config-only": "node ../scripts/setup-wizard.js --config-only",
    "test:e2e:error-recovery:test": "node ../scripts/enhanced-error-recovery.js --test-error",
    "test:setup-dirs": "node ../scripts/setup-test-directories.js",
    "test:setup-dirs:clean": "node ../scripts/setup-test-directories.js --clean"
  },
  "dependencies": {
    "@a2a-js/sdk": "^0.2.4",
    "@modelcontextprotocol/sdk": "^1.9.0",
    "@radix-ui/react-accordion": "^1.2.8",
    "@radix-ui/react-checkbox": "^1.1.5",
    "@radix-ui/react-collapsible": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.7",
    "@radix-ui/react-dropdown-menu": "^2.1.7",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-popover": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.2.4",
    "@radix-ui/react-scroll-area": "^1.2.4",
    "@radix-ui/react-select": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.3",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-tabs": "^1.1.4",
    "@radix-ui/react-tooltip": "^1.2.0",
    "@types/uuid": "^10.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "framer-motion": "^12.6.3",
    "lucide-react": "^0.487.0",
    "next": "15.3.0",
    "next-themes": "^0.4.6",
    "postcss": "^8.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "tw-animate-css": "^1.2.5",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "commander": "^14.0.0",
    "cypress": "^14.5.2",
    "eslint": "^9",
    "eslint-config-next": "15.3.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "glob": "^11.0.3",
    "tailwindcss": "^4.1.3",
    "typescript": "^5"
  },
  "files": [
    "bin/",
    "public/",
    "src/",
    "next.config.js",
    "postcss.config.js",
    "tailwind.config.js",
    "tsconfig.json"
  ],
  "keywords": [
    "agentgateway",
    "gateway",
    "ui",
    "nextjs"
  ],
  "author": "",
  "license": "MIT"
}
