---
source: crates/agentgateway/src/types/local.rs
description: "Config normalization test for multiplex: YAML -> LocalConfig -> NormalizedLocalConfig -> YAML"
---
binds:
- key: bind/3000
  address: '[::]:3000'
  listeners:
    listener0/bind/3000:
      key: listener0/bind/3000
      name: listener0
      gatewayName: bind/3000
      hostname: ''
      protocol: HTTP
      routes:
        listener0/bind/3000/route0/default:
          key: listener0/bind/3000/route0/default
          routeName: route0
          matches:
          - path:
              pathPrefix: /
          backends:
          - weight: 1
            backend: listener0/bind/3000/route0/default
          policies:
            timeout: {}
            retry: null
      tcpRoutes: {}
policies: []
backends:
- mcp:
    name: listener0/bind/3000/route0/default
    target:
      targets:
      - name: time
        stdio:
          cmd: uvx
          args:
          - mcp-server-time
      - name: everything
        stdio:
          cmd: npx
          args:
          - '@modelcontextprotocol/server-everything'
workloads: []
services: []
